{
    "manifest_version": 2,
    "name": "YouTube Blocker",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}